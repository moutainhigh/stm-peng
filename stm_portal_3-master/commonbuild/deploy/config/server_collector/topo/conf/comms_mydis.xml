<?xml version="1.0" encoding="utf-8"?>
<!-- 
	端口 port
	单个设备ip地址 ip
	snmp版本 version：	v1:0,v2:1,v3:3,配置对应的数字 
	读团体字 readComm
	写团体字 writeComm
	采集类型 collectType
	重试次数 retryTimes
	超时时间 timeout
	
	SNMP V3参数
	安全级别 secLevel:   AUTH_PRIV:3,AUTH_NOPRIV:2,NOAUTH_NOPRIV:1,配置对应的数字
	用户名 secName
	鉴别协议 authProt:	MD5,SHA
	鉴别密码 authPwd
	加密协议 privateProt:	DES,3DES,AES128,AES192,AES256 
	加密密码 privatePwd
	上下文 contextName
 -->
<config protocol="ROUTE">
  <snmps>
    <snmp port="161" ip="0.0.0.0" version="1" readComm="public" collectType="GET" retryTimes="1" timeout="1000" secLevel="1"/>
  </snmps>
</config>
