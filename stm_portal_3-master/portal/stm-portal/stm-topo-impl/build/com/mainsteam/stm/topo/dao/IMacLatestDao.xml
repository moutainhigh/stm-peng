  <update id="updateByMac" parameterType="java.util.Map">
	   UPDATE STM_TOPO_LATEST_MAC
	   SET host_name = #{hostName}
	   WHERE mac = #{mac} and host_name != #{hostName}
	</update>
   
   
   
   
   
