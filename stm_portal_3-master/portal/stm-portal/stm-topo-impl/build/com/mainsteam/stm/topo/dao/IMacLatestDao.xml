		SELECT * FROM STM_TOPO_LATEST_MAC
		 
		 
     
     
     
     
     
    <if test="condition != null and condition.searchType !=null and condition.searchVal != null and condition.searchVal != ''">
			 
			 
       
       
       
       
       
