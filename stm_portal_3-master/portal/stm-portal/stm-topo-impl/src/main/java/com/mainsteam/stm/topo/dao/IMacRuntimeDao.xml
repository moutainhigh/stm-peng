  <select id="selectByHostIp" parameterType="java.util.Map" resultMap="macRuntimeMap" databaseId="mysql">
		SELECT * FROM STM_TOPO_MAC_RUNTIME WHERE FIND_IN_SET(#{ip},ip)
	</select>
   
   
   
   
   
