<?xml version="1.0" encoding="UTF-8"?>
<Plugin version="1.0" id="IceGridPlugin" name="IceGridPlugin" class="com.mainsteam.stm.plugin.icegrid.IceGridPluginSession" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../schema/Plugin.xsd">
	<MaxActiveSession>1</MaxActiveSession>
	<MaxidleSession>0</MaxidleSession>
	<SessionTTL>10000</SessionTTL>
	<ObjectCacheTimeout>30000</ObjectCacheTimeout>

	<PluginInitParameters>

		<parameter id="IceGridGrpBox" boxStyle="GroupBox" name="IceGrid连接信息" displayOrder="1" isEdit="false" isDisplay="true" isSessionKey="false">
			<DefaultValue />
			<HelpInfo>IceGrid连接信息</HelpInfo>
		</parameter>

		<parameter id="IceEndpoints" boxStyle="Input" name="端点" displayOrder="1" isEdit="true" isDisplay="true" isSessionKey="true" mustInput="true">
			<DefaultValue></DefaultValue>
			<HelpInfo>IceGrid端点，如：default -h 192.168.10.16 -p 4061</HelpInfo>
		</parameter>

		<parameter id="IceInstanceName" boxStyle="Input" name="实例名称" displayOrder="2" isEdit="true" isDisplay="true" mustInput="true" isSessionKey="true">
			<HelpInfo>实例名称</HelpInfo>
		</parameter>

		<parameter id="IceUsername" boxStyle="Input" name="用户名" displayOrder="3" isEdit="true" isDisplay="true" mustInput="true" isSessionKey="true">
			<HelpInfo>用户名</HelpInfo>
		</parameter>
		
		<parameter id="IcePassword" boxStyle="Input" name="密码" displayOrder="4" isEdit="true" isDisplay="true" mustInput="true" isSessionKey="true">
			<HelpInfo>密码</HelpInfo>
		</parameter>
	</PluginInitParameters>
</Plugin>
