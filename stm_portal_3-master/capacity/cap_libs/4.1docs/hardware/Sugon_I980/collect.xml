            <PluginDataHandler classKey="translateProcessor">
                <Parameter type="" key="" value="false,正常;异常"/>
            </PluginDataHandler>
        </PluginDataHandlers>
    </MetricPlugin>

    <MetricPlugin metricid="PoweControFault" collectType="" pluginid="IpmiPlugin">
        <PluginResultMetaInfo columns=""/>
