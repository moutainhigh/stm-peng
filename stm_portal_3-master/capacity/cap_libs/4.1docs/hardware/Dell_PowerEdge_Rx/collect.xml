    <MetricPlugin metricid="PowerStatus" collectType="" pluginid="IpmiPlugin">
        <PluginResultMetaInfo columns=""/>
        <PluginParameter type="ArrayType">
            <Parameter key="tool" value="ipmitool"/>
            <Parameter key="command" value="power status"/>
