        <PluginDataHandlers>
            <PluginDataHandler classKey="RegularFilter">
                <Parameter type="" key="REGULAR" value="\bFAN_+(\d+)\s+\S\s+\S+\s+\S+\s+(\S+)"/>
            </PluginDataHandler>
            <PluginDataHandler classKey="translateProcessor">
                <Parameter type="" key="fanStatus" value="ok,1;0"/>
