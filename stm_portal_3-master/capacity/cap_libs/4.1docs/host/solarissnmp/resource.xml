                </Thresholds>
            </Metric>
        </Metrics>
        <Properties>
            <Property id="ifIndex" name="ifIndex" metricid="ifIndex"/>
            <Property id="ifName" name="ifName" metricid="ifName"/>
            <Property id="ifSpeed" name="ifSpeed" metricid="ifSpeed"/>
            <Property id="availability" name="availability" metricid="ifOperStatus"/>
        </Properties>
        <Instantiation InstanceId="ifIndex" InstanceName="ifName"/>
    </Resource>
</Capacity>
