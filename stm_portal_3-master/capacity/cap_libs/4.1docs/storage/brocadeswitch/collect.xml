    <MetricPlugin metricid="throughput" collectType="SNMP" pluginid="SnmpPlugin">
        <PluginResultMetaInfo columns="throughputIndex,ifInOctets_ifHCInOctets"/>
        <PluginParameter type="ArrayType">
            <Parameter type="" key="method" value="getBulk"/>
            <Parameter type="" key="COUNTER64" value="1.3.6.1.2.1.31.1.1.1.6"/>
            <Parameter type="" key="COUNTER32" value="1.3.6.1.2.1.2.2.1.10"/>
        </PluginParameter>
        <PluginDataHandlers>
            <PluginDataHandler classKey="ChangeRateProcessor">
                <Parameter type="" key="COLUMNINDEX" value="throughputIndex"/>
                <Parameter type="" key="FUNCTION" value="ROUND_CHGV"/>
                <Parameter type="" key="COUNTER32" value="2^32">
                    <!--增加的模值。一般用于网络接口中，它使用的是类似counter32或者counter64等计数，当达到阈值时会重置为0，然后再进行计算。为了计算准确性，需要加入一个模量再对比前后两次的值进行增量计算。-->
