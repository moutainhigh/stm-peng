    <MetricPlugin metricid="ifInOctetsSpeed" collectType="SNMP" pluginid="SnmpPlugin">
        <PluginResultMetaInfo columns="ifIndex,ifInOctets_ifHCInOctets"/>
        <PluginParameter type="ArrayType">
            <Parameter type="" key="method" value="getBulk"/>
            <Parameter type="" key="" value="1.3.6.1.4.1.1588.2.1.1.1.6.2.1.12"/>
        </PluginParameter>
        <PluginDataHandlers>
            <PluginDataHandler classKey="RowFilterByInstPropertyProcessor">
                <Parameter type="" key="IndexColumnTitle" value="ifIndex"/>
                <Parameter type="ResourceProperty" key="InstPropertyKey" value="ifIndex"/>
            </PluginDataHandler>
            <PluginDataHandler classKey="ChangeRateProcessor">
                <Parameter type="" key="COLUMNINDEX" value="ifIndex"/>
                <Parameter type="" key="FUNCTION" value="ROUND_CHGV"/>
                <Parameter type="" key="MOD" value="2^32">
                    <!--增加的模值。一般用于网络接口中，它使用的是类似counter32或者counter64等计数，当达到阈值时会重置为0，然后再进行计算。为了计算准确性，需要加入一个模量再对比前后两次的值进行增量计算。-->
                </Parameter>
            </PluginDataHandler>
            <PluginDataHandler classKey="selectProcessor">
                <Parameter type="" key="SELECT" value="SELECT ifIndex,(ifInOctets_ifHCInOctets*4) as ifInOctetsSpeed"/>
            </PluginDataHandler>
        </PluginDataHandlers>
        <PluginDataConverter classKey="subInstConverter">
            <Parameter type="" key="IndexColumnTitle" value="ifIndex"/>
            <Parameter type="" key="ValueColumnTitle" value="ifInOctetsSpeed"/>
            <Parameter type="ResourceProperty" key="InstPropertyKey" value="ifIndex"/>
        </PluginDataConverter>
    </MetricPlugin>
