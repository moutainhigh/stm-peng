    <MetricPlugin metricid="ReadIOPS" collectType="SNMP" pluginid="SnmpPlugin">
        <PluginResultMetaInfo columns="readIndex,readDatas"/>
        <PluginParameter type="ArrayType">
            <Parameter type="" key="method" value="walk"/>
            <Parameter type="" key="" value="1.3.6.1.4.1.9804.3.1.1.2.4.16"/>
        </PluginParameter>
        <PluginDataHandlers>
            <PluginDataHandler classKey="ChangeRateProcessor">
                <Parameter type="" key="COLUMNINDEX" value="readIndex"/>
                <Parameter type="" key="FUNCTION" value="ROUND_CHGV"/>
                <Parameter type="" key="MOD" value="2^64">
                    <!--增加的模值。一般用于网络接口中，它使用的是类似counter32或者counter64等计数，当达到阈值时会重置为0，然后再进行计算。为了计算准确性，需要加入一个模量再对比前后两次的值进行增量计算。-->
                </Parameter>
            </PluginDataHandler>
            <PluginDataHandler classKey="selectProcessor">
                <Parameter type="" key="SELECT" value="SELECT first(readDatas) as ReadIOPS"/>
            </PluginDataHandler>
        </PluginDataHandlers>
    </MetricPlugin>
