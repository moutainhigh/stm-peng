<?xml version="1.0" encoding="UTF-8"?>

<Capacity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:noNamespaceSchemaLocation="../../../schema/Resource.xsd">
    <SupportInfo>
        <SupportName>Niginx HTTP Server</SupportName>
        <PreSupportVersion>Niginx 2.2.2</PreSupportVersion>
        <ActualSupportVersion>Windows 2003/2.2.2</ActualSupportVersion>
