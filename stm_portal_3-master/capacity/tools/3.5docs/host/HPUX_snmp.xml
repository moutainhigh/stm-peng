<?xml version="1.0" encoding="UTF-8"?>

<root> 
  <!-- 
	devicetype:			设备类型名称
	ismulticoltype:		是否允许采集方式多选
	majortype:			主要分类（可以理解为设备类型）
	minortype:			次要分类
	manufacturer:		厂商
	devicetype_zh:		设备类型中文描述
	 -->  
  <devicetypeinfo devicetype="HPUX_snmp" ismulticoltype="true" majortype="host" minortype="HPUnix" manufacturer="" devicetype_zh="HPUX操作系统(snmp)"> 
    <!-- 用于定位设备为该类设备的多个sysoid标识，如果snmp采集为sysoid，如果其他方式自行定义 -->  
    <devicetypes> 
      <devicetype sysoid="1.3.6.1.4.1.11.2.3.2.3"/>  
      <devicetype sysoid="1.3.6.1.4.1.8072.3.2.14"/> 
    </devicetypes>  
    <!-- 该设备类型定义的命令，用于合并命令执行 -->  
    <commands> 
      <!-- coltype:采集方式，comid：命令ID，command：命令 -->  
      <command coltype="SNMP" comid="systemUptime" command="1.3.6.1.2.1.1.3.0"/>  
      <!--连续运行时间 -->  
      <command coltype="SNMP" comid="cpuRate" command="1.3.6.1.4.1.2021.11.11"/>  
      <!--CPU平均利用率 -->  
      <command coltype="SNMP" comid="systemInfo" command="1.3.6.1.2.1.1.1"/>  
      <!--主机基本信息 -->  
      <command coltype="SNMP" comid="systemOID" command="1.3.6.1.2.1.1.2"/>  
      <!--主机操作系统的OID -->  
      <command coltype="SNMP" comid="cotainedTopoNodeIP" command="1.3.6.1.2.1.4.20.1.1"/>  
      <!--主机的所有IP地址 -->  
      <command coltype="SNMP" comid="osVersion" command="1.3.6.1.2.1.1.1"/>  
      <!--主机操作系统版本 -->  
      <command coltype="SNMP" comid="totalPgSpaceSize" command="1.3.6.1.4.1.2021.4.3"/>  
      <!--Paging Space总大小 -->  
      <command coltype="SNMP" comid="freePgSpaceSize" command="1.3.6.1.4.1.2021.4.4"/>  
      <!--Paging Space可用大小 -->  
      <command coltype="SNMP" comid="cPUUserModePer" command="1.3.6.1.4.1.2021.11.9"/>  
      <!--CPU用户模式百分比 -->  
      <command coltype="SNMP" comid="cPUIdlePer" command="1.3.6.1.4.1.2021.11.11"/>  
      <!--CPU空闲时间百分比 -->  
      <command coltype="SNMP" comid="cPUSysModePercent" command="1.3.6.1.4.1.2021.11.10"/>  
      <!--CPU系统模式百分比 -->  
      <command coltype="SNMP" comid="numberOfNIC" command="1.3.6.1.2.1.2.2.1.3"/>  
      <!--网络端口个数 -->  
      <command coltype="SNMP" comid="hostname" command="1.3.6.1.2.1.1.5"/>  
      <!--主机名 -->  
      <command coltype="SNMP" comid="outICMPPacketRate" command="1.3.6.1.2.1.5.14"/>  
      <!--发送的ICMP包率 -->  
      <command coltype="SNMP" comid="iifMPPacketRate" command="1.3.6.1.2.1.5.1"/>  
      <!--接收的ICMP包率-->  
      <command coltype="INNER_PROC" comid="testAvail" command="com.qwserv.itm.pfl.resource.util.TestConnection.testAvail"/>  
      <!-- 测试是否可连接 -->  
      <command coltype="INNER_PROC" comid="testPing" command="com.qwserv.itm.pfl.resource.util.TestConnection.testPing"/>  
      <!-- 测试是否可ping -->  
      <!-- 端口子资源 -->  
      <command coltype="SNMP" comid="ifID" command="1.3.6.1.2.1.2.2.1.1"/>  
      <!--网卡ID号/端口索引 -->  
      <command coltype="SNMP" comid="ifName" command="1.3.6.1.2.1.2.2.1.2"/>  
      <!--端口名称 -->  
      <command coltype="SNMP" comid="ifMAC" command="1.3.6.1.2.1.2.2.1.6"/>  
      <!--MAC地址 -->  
      <command coltype="SNMP" comid="ifType" command="1.3.6.1.2.1.2.2.1.3"/>  
      <!--端口类型 -->  
      <command coltype="SNMP" comid="ifBandwidth" command="1.3.6.1.2.1.2.2.1.5"/>  
      <!--端口带宽 -->  
      <command coltype="SNMP" comid="ifAdminState" command="1.3.6.1.2.1.2.2.1.7"/>  
      <!--管理状态 -->  
      <command coltype="SNMP" comid="ifOperState" command="1.3.6.1.2.1.2.2.1.8"/>  
      <!--操作状态 -->  
      <command coltype="SNMP" comid="ifInRate" command="1.3.6.1.2.1.2.2.1.10"/>  
      <!--接收速率(Kbps) -->  
      <command coltype="SNMP" comid="ifOutRate" command="1.3.6.1.2.1.2.2.1.16"/>  
      <!--发送速率(Kbps) -->  
      <!--分区子资源 -->  
      <command coltype="SNMP" comid="fileSysType" command="1.3.6.1.2.1.25.2.3.1.2"/>  
      <!--分区类型 -->  
      <command coltype="SNMP" comid="fileSysName" command="1.3.6.1.2.1.25.2.3.1.3"/>  
      <!--分区名称 -->  
      <command coltype="SNMP" comid="fileSysSize" command="1.3.6.1.2.1.25.2.3.1.5"/>  
      <!--分区容量-->  
      <command coltype="SNMP" comid="fileSysUnit" command="1.3.6.1.2.1.25.2.3.1.4"/>  
      <!--分区单位 -->  
      <command coltype="SNMP" comid="fileSysUsedSize" command="1.3.6.1.2.1.25.2.3.1.6"/>  
      <!--分区已用容量 -->  
      <!-- snmp内存总量 -->  
      <command coltype="SNMP" comid="snmpHostMemorySize" command="1.3.6.1.4.1.2021.4.5.0"/>  
      <!-- snmp内存使用量 -->  
      <command coltype="SNMP" comid="snmpHostMemoryFreeSize" command="1.3.6.1.4.1.2021.4.6.0"/>  
    </commands>  
    <!-- 描述该类设备的所有指标采集,一行对应一个指标 -->  
    <!-- kpiid的命名规范：长度不超过20，命名规范，前两位为模块标识，分别以以下字符打头， N（网络）、H（主机）、D（数据库）、M（中间件）、A（应用）；第二位为数字，由模块内部进行标识定义-->  
    <resources> 
      
      <!--内存总容量 -->  
      <resource kpiid="H1totalMemSize" coltype="SNMP" value="$d1/1024/1024" valuetype="Float" unit="GB"> 
        <collects> 
          <collect exeorder="10" comid="snmpHostMemorySize" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1totalMemSize" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="${value}!=${oldvalue}" alertdesc2="内存总容量发生变化，从${oldvalue}MB变更为${value}MB。" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="1"/> 
      </resource>  
      <!--主机总已用内存容量 -->  
      <resource kpiid="H1memPoolUesd" coltype="SNMP" value="($d2-$d1)/1024/1024" valuetype="Float" unit="GB"> 
        <collects> 
          <collect exeorder="10" comid="snmpHostMemoryFreeSize" select="" merge="" deal=""/> 
          <collect exeorder="20" comid="snmpHostMemorySize" select="" merge="" deal=""/>
        </collects>  
        <kpipolicy table="res_d_H1memPoolUesd" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="5" storefactor="1"/> 
      </resource>  
      <!--内存利用率 -->  
      <resource kpiid="H1memUsage" coltype="SNMP" value="($d2-$d1)/$d2*100" valuetype="Float" unit="%"> 
        <collects> 
          <collect exeorder="10" comid="snmpHostMemoryFreeSize" select="" merge="" deal=""/> 
          <collect exeorder="20" comid="snmpHostMemorySize" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1memUsage" field="value" fieldtype="" willalert="y" alertexp1="${value}&lt;80" alertdesc1="内存利用率恢复正常，当前值为${value}%。" alertexp2="" alertdesc2="" alertexp3="${value}&gt;=80" alertdesc3="内存利用率大于等于${trigervalue1}%，当前值为${value}%。" alertexp4="${value}&gt;=90" alertdesc4="内存利用率大于等于${trigervalue1}%，当前值为${value}%。" alertexp5="" alertdesc5="" alertcontin="1" circle="5" storefactor="1"/> 
         </resource>  
      <!-- 主机在线可用性，snmpping\icmp ping都不通则不可用，否则可用 -->  
      <resource kpiid="Availability" coltype="INNER_PROC" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="testAvail" select="" merge="" deal=""/> 
        </collects>  
        <translations> 
          <translation source="-1" dest="不可用"/>  
          <translation source="0" dest="可用"/> 
        </translations>  
        <kpipolicy table="res_d_Availability" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="设备连接正常" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="&quot;${value}&quot;.contains(&quot;不可用&quot;)" alertdesc5="设备连接失败" alertcontin="1" circle="5" storefactor="1"/> 
      </resource>  
      <!-- 设备ICMP Ping 丢包率   
      <resource kpiid="H1PingLostRate" coltype="INNER_PROC" value="" valuetype="Float" unit="%"> 
        <collects> 
          <collect exeorder="10" comid="testPing" select="lost\t(\d+)" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1PingLostRate" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1" storefactor="1"/> 
      </resource>  -->
      <!-- 设备Ping时延 -->  
      <resource kpiid="H1PingResponseTime" coltype="INNER_PROC" value="" valuetype="Integer" unit="毫秒"> 
        <collects> 
          <collect exeorder="10" comid="testPing" select="delay\t([\-\d]+)" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1ResponseTime" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1" storefactor="1"/> 
      </resource>  
      <!-- 连续运行时间 -->  
      <resource kpiid="H1sysUpTime" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="systemUptime" select="([^\.]+)\." merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1sysUpTime" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--CPU平均利用率-->  
      <resource kpiid="H1cpuUsage" coltype="SNMP" value="100-$d1" valuetype="Integer" unit="%"> 
        <collects> 
          <collect exeorder="10" comid="cpuRate" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1cpuUsage" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="5" storefactor="1"/> 
      </resource>  
      <!--主机基本信息 -->  
      <resource kpiid="H1systemInfo" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="systemInfo" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1systemInfo" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="0"/> 
      </resource>  
      <!--主机操作系统的OID -->  
      <resource kpiid="H1systemOID" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="systemOID" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1systemOID" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="0"/> 
      </resource>  
      <!--主机的所有IP地址 -->  
      <resource kpiid="H1allIpAddress" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="cotainedTopoNodeIP" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1allIpAddress" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="60" storefactor="1"/> 
      </resource>  
      <!--主机操作系统版本 -->  
      <resource kpiid="H1osVersion" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="osVersion" select="hpux\s+([^\s]+)\s+" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1osVersion" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="1"/> 
      </resource>  
      <!--Paging Space总大小 -->  
      <resource kpiid="H1totalPSSize" coltype="SNMP" value="" valuetype="Float" unit="KB"> 
        <collects> 
          <collect exeorder="10" comid="totalPgSpaceSize" select="(\d+)" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1totalPSSize" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--Paging Space已用大小 -->  
      <resource kpiid="H1usedPSSize" coltype="SNMP" value="$d1-$d2" valuetype="Float" unit="KB"> 
        <collects> 
          <collect exeorder="10" comid="totalPgSpaceSize" select="(\d+)" merge="" deal=""/>  
          <collect exeorder="20" comid="freePgSpaceSize" select="(\d+)" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1usedPSSize" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--CPU用户模式百分比 -->  
      <resource kpiid="H1cpuUserModeTimePer" coltype="SNMP" value="" valuetype="Integer" unit="%"> 
        <collects> 
          <collect exeorder="10" comid="cPUUserModePer" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1cpuUserModeTimePer" field="value" fieldtype="" willalert="y" alertexp1="${value}&lt;50" alertdesc1="CPU用户模式百分比恢复正常，当前值为${value}%。" alertexp2="" alertdesc2="" alertexp3="${value}&gt;=50" alertdesc3="CPU用户模式百分比大于等于${trigervalue1}%，当前值为${value}%。" alertexp4="${value}&gt;=80" alertdesc4="CPU用户模式百分比大于等于${trigervalue1}%，当前值为${value}%。" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--CPU空闲时间百分比 -->  
      <resource kpiid="H1cpuIdleTimePer" coltype="SNMP" value="" valuetype="Integer" unit="%"> 
        <collects> 
          <collect exeorder="10" comid="cPUIdlePer" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1cpuIdleTimePer" field="value" fieldtype="" willalert="y" alertexp1="${value}&gt;30" alertdesc1="CPU空闲时间百分比恢复正常，当前值为${value}%。" alertexp2="" alertdesc2="" alertexp3="${value}&lt;30" alertdesc3="CPU空闲时间百分比小于等于${trigervalue1}%，当前值为${value}%。" alertexp4="${value}&lt;10" alertdesc4="CPU空闲时间百分比小于等于${trigervalue1}%，当前值为${value}%。" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--CPU系统模式百分比 -->  
      <resource kpiid="H1cpuSysModeTimePer" coltype="SNMP" value="" valuetype="Integer" unit="%"> 
        <collects> 
          <collect exeorder="10" comid="cPUSysModePercent" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1cpuSysModeTimePer" field="value" fieldtype="" willalert="y" alertexp1="${value}&lt;50" alertdesc1="CPU用户模式百分比恢复正常，当前值为${value}%。" alertexp2="" alertdesc2="" alertexp3="${value}&gt;=50" alertdesc3="CPU系统模式百分比大于等于${trigervalue1}%，当前值为${value}%。" alertexp4="${value}&gt;=80" alertdesc4="CPU系统模式百分比大于等于${trigervalue1}%，当前值为${value}%。" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--网络端口个数 -->  
      <resource kpiid="H1ifNum" coltype="SNMP" value="" valuetype="Integer" unit="个"> 
        <collects> 
          <collect exeorder="10" comid="numberOfNIC" select="(6)" merge="count" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1ifNum" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="${value}!=${oldvalue}" alertdesc2="网络端口个数发生变化，从${oldvalue}个变更为${value}个。" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="1"/> 
      </resource>  
      <!--主机名 -->  
      <resource kpiid="H1hostName" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="hostname" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1hostName" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="!&quot;${value}&quot;.equals(&quot;${oldvalue}&quot;)" alertdesc2="主机名发生变化，从${oldvalue}变更为${value}。" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="1"/> 
      </resource>  
      <!--发送的ICMP包率 -->  
      <resource kpiid="H1icmpOutMsgsRate" coltype="SNMP" value="" valuetype="Float" unit="包/秒"> 
        <collects> 
          <collect exeorder="10" comid="outICMPPacketRate" select="" merge="sum" deal="chgv"/> 
        </collects>  
        <kpipolicy table="res_d_H1icmpOutMsgsRate" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--接收的ICMP包率 -->  
      <resource kpiid="H1icmpInMsgsRate" coltype="SNMP" value="" valuetype="Float" unit="包/秒"> 
        <collects> 
          <collect exeorder="10" comid="iifMPPacketRate" select="" merge="sum" deal="chgv"/> 
        </collects>  
        <kpipolicy table="res_d_H1icmpInMsgsRate" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!--接收和发送的ICMP包率 -->  
      <resource kpiid="H1icmpMsgsRate" coltype="SNMP" value="$d1+$d2" valuetype="Float" unit="包/秒"> 
        <collects> 
          <collect exeorder="10" comid="outICMPPacketRate" select="" merge="sum" deal="chgv"/>  
          <collect exeorder="20" comid="iifMPPacketRate" select="" merge="sum" deal="chgv"/> 
        </collects>  
        <kpipolicy table="res_d_H1icmpMsgsRate" field="value" fieldtype="" willalert="y" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="15" storefactor="1"/> 
      </resource>  
      <!-- 端口子资源 -->  
      <!--端口ID  
      <resource kpiid="H1ifID" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="ifID" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1ifID" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="1"/> 
      </resource>  --> 
      <!--索引 -->  
      <resource kpiid="H1ifIndex" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="ifID" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1ifIndex" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="30" storefactor="1"/> 
      </resource>  
      <!--端口名称 -->  
      <resource kpiid="H1ifName" coltype="SNMP" value="" valuetype="String" unit=""> 
        <collects> 
          <collect exeorder="10" comid="ifName" select="" merge="" deal=""/> 
        </collects>  
        <kpipolicy table="res_d_H1ifName" field="value" fieldtype="" willalert="n" alertexp1="" alertdesc1="" alertexp2="" alertdesc2="" alertexp3="" alertdesc3="" alertexp4="" alertdesc4="" alertexp5="" alertdesc5="" alertcontin="1" circle="1440" storefactor="1"/> 
      </resource>  
      <!--端口类型 -->  
      <resource kpiid="H1ifType" coltype="SNMP" value="" valuetype="String" unit=""> 
