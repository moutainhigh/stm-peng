<?xml version="1.0" encoding="UTF-8"?>
<Capacity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:noNamespaceSchemaLocation="../../../schema/Resource.xsd">
    <SupportInfo>
        <SupportName>Cisco Media Convergence Server</SupportName>
        <PreSupportVersion>Cisco MCS 7800系列</PreSupportVersion>
        <ActualSupportVersion>Cisco MCS 7845</ActualSupportVersion>
    </SupportInfo>
    <GlobalMetricSetting isEncrypt="FALSE" version="4.1.0">
        <GlobalIsEdit>false</GlobalIsEdit>
        <GlobalIsMonitor>true</GlobalIsMonitor>
        <GlobalDefaultMonitorFreq>day1</GlobalDefaultMonitorFreq>
        <GlobalsupportMonitorFreq>min1,min5,min10,hour1,day1</GlobalsupportMonitorFreq>
        <GlobalIsAlert>true</GlobalIsAlert>
        <GlobalDefaultFlapping>1</GlobalDefaultFlapping>
    </GlobalMetricSetting>
    <Resource id="CiscoMCS" category="MediaConvergenceServer" icon="MediaConvergenceServer.png" name="Cisco MCS" description="Cisco MCS">
        <Metrics>
            <Metric id="availability" style="AvailabilityMetric" name="可用性" description="可用性" unit="">
                <IsDisplay displayOrder="1000">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>min1</DefaultMonitorFreq>
                <SupportMonitorFreq>sec30,min1,min5,min10,min30,hour1</SupportMonitorFreq>
                <IsAlert>true</IsAlert>
                <DefaultFlapping>2</DefaultFlapping>
            </Metric>
            <Metric id="hostname" name="主机名" style="InformationMetric" description="主机名" unit="">
                <IsDisplay displayOrder="1100">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="date" name="系统时间" style="InformationMetric" description="系统时间" unit="">
                <IsDisplay displayOrder="1110">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="timezone" name="时区" style="InformationMetric" description="时区" unit="">
                <IsDisplay displayOrder="1120">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="locale" name="编码" style="InformationMetric" description="编码" unit="">
                <IsDisplay displayOrder="1130">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="productVersion" name="产品版本" style="InformationMetric" description="产品版本" unit="">
                <IsDisplay displayOrder="1140">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <!--<Metric id="platformVersion" name="平台版本" style="InformationMetric" description="平台版本" unit="">
                <IsDisplay displayOrder="1150">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>-->
            <Metric id="licenseMAC" name="许可证MAC" style="InformationMetric" description="许可证MAC" unit="">
                <IsDisplay displayOrder="1160">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="uptime" name="运行时间" style="InformationMetric" description="运行时间" unit="s">
                <IsDisplay displayOrder="1170">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="cpuRate" style="PerformanceMetric" name="CPU利用率" description="CPU利用率" unit="%">
                <IsDisplay displayOrder="1500">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>min5</DefaultMonitorFreq>
                <SupportMonitorFreq>min1,min5,min10,min30,hour1,hour4,hour8</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
                <Thresholds>
                    <Threshold operator="&lt;" defaultvalue="80" stateid="Normal"/>
                    <Threshold operator="&gt;" defaultvalue="85" stateid="Minor"/>
                    <Threshold operator="&gt;" defaultvalue="95" stateid="Major"/>
                </Thresholds>
            </Metric>
            <Metric id="memRate" style="PerformanceMetric" name="内存利用率" description="内存利用率" unit="%">
                <IsDisplay displayOrder="1510">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>min5</DefaultMonitorFreq>
                <SupportMonitorFreq>min1,min5,min10,min30,hour1,hour4,hour8</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
                <Thresholds>
                    <Threshold operator="&lt;" defaultvalue="80" stateid="Normal"/>
                    <Threshold operator="&gt;" defaultvalue="85" stateid="Minor"/>
                    <Threshold operator="&gt;" defaultvalue="95" stateid="Major"/>
                </Thresholds>
            </Metric>
        </Metrics>
        <Properties>
            <Property id="hostname" name="hostname" metricid="hostname"/>
        </Properties>
        <Instantiation InstanceId="hostname" InstanceName="hostname"/>
    </Resource>
    <Resource id="CiscoMCSDisk" type="Disk" icon="" name="磁盘" description="磁盘" parentid="CiscoMCS">
        <Metrics>
            <Metric id="diskName" style="InformationMetric" name="名称" description="名称" unit="">
                <IsDisplay displayOrder="2000">false</IsDisplay>
                <IsMonitor>false</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>day1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="diskTotal" style="InformationMetric" name="总容量" description="总容量" unit="KB">
                <IsDisplay displayOrder="2100">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>hour1</DefaultMonitorFreq>
                <SupportMonitorFreq>hour1,day1</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="diskUsage" style="PerformanceMetric" name="利用率" description="利用率" unit="%">
                <IsDisplay displayOrder="2200">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>min5</DefaultMonitorFreq>
                <SupportMonitorFreq>min1,min5,min10,min30,hour1,hour4,hour8</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
                <Thresholds>
                    <Threshold operator="&lt;" defaultvalue="80" stateid="Normal"/>
                    <Threshold operator="&gt;=" defaultvalue="85" stateid="Minor"/>
                    <Threshold operator="&gt;=" defaultvalue="90" stateid="Major"/>
                </Thresholds>
            </Metric>
            <Metric id="diskFree" style="InformationMetric" name="空闲容量" description="空闲容量" unit="KB">
                <IsDisplay displayOrder="2210">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>min5</DefaultMonitorFreq>
                <SupportMonitorFreq>min1,min5,min10,min30,hour1,hour4,hour8</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
            <Metric id="diskUsed" style="InformationMetric" name="已用容量" description="已用容量" unit="KB">
                <IsDisplay displayOrder="2220">true</IsDisplay>
                <IsMonitor>true</IsMonitor>
                <IsEdit>false</IsEdit>
                <DefaultMonitorFreq>min5</DefaultMonitorFreq>
                <SupportMonitorFreq>min1,min5,min10,min30,hour1,hour4,hour8</SupportMonitorFreq>
                <IsAlert>false</IsAlert>
                <DefaultFlapping>1</DefaultFlapping>
            </Metric>
        </Metrics>
        <Properties>
            <Property id="diskName" name="diskName" metricid="diskName"/>
        </Properties>
        <Instantiation InstanceId="diskName" InstanceName="diskName"/>
    </Resource>
</Capacity>
