                    <Threshold operator="&gt;" defaultvalue="720" stateid="Normal"/>
                    <Threshold operator="&gt;" defaultvalue="12000" stateid="Minor"/>
                    <Threshold operator="&gt;" defaultvalue="21000" stateid="Major"/>
                </Thresholds>
            </Metric>
        </Metrics>
