  <GlobalMetricSetting isEncrypt="FALSE" version="4.1.0"/>  
  <PluginClassAliasList> 
    <PluginClassAlias id="defaultProcessor" class="com.mainsteam.stm.plugin.common.DefaultResultSetProcessor"/>
