       <MetricPlugin metricid="StorageAdapterId" collectType="" pluginid="vmPlugin"> 
    <PluginResultMetaInfo columns="StorageAdapterId"/>  
    <PluginParameter type="ArrayType"> 
     <Parameter type="" key="COMMAND" value="com.mainsteam.stm.plugin.vmware.collector.VMWareESXICollector.getStorageAdapterList"/>
    </PluginParameter>
   <PluginDataHandlers>
      <PluginDataHandler classKey="RegularFilter">
        <Parameter value="uuid" key="uuid" type="ResourceProperty"/> 
        <Parameter type="" key="REGULAR" value="&lt;${uuid}&gt;(.+)&lt;/${uuid}&gt;"/>
      </PluginDataHandler>
            <PluginDataHandler classKey="RegularFilter">
                      <Parameter value="&lt;StorageAdapter>(\S+)\s+\S+\s+\S+\s+\S+\s+\S+\s+\S+&lt;/StorageAdapter>"
                    key="REGULAR" />
            </PluginDataHandler>
