        <PluginDataConverter classKey="subInstConverter">
            <Parameter type="" key="IndexColumnTitle" value="StorageAdapterId" />
            <Parameter type="" key="ValueColumnTitle" value="StorageAdapterId" />
        </PluginDataConverter> 
  </MetricPlugin>
  
